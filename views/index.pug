doctype html
html
    head
        meta(charset='UTF-8')
        title feed-mamp
        link(rel='stylesheet', href='/styles.css')

div.container
    form(action='/addtask' method='POST')
        fieldset
            h2 Add new Feeds
            input#newtask(type='text' name='newfeed' placeholder='add new feed')
            br
            button(type='submit') Add Feed
            br
            h2 Feeds
            p 
                each feed in feeds
                    ul
                        li= feed.feedkey